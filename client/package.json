{
  "name": "pwa-rss-reader-client",
  "version": "1.0.0",
  "proxy": "http://localhost:5000/",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:app && npm run build:sw",
    "build:app": "react-scripts build",
    "build:sw": "webpack src/sw/index.js --output build/service-worker.js --mode=production --optimize-max-chunks 1 --module-bind js=babel-loader --devtool source-map",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:cov": "react-scripts test --watchAll=false --coverage",
    "format": "prettier src/**/*.{ts,tsx,js,jsx} --write",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "axios": "^0.19.1",
    "bootstrap": "^4.4.1",
    "idb": "^4.0.5",
    "immer": "^5.2.1",
    "normalizr": "^3.4.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-waypoint": "^9.0.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "register-service-worker": "^1.6.2",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.2.1",
    "typescript": "~3.7.4",
    "webpack-cli": "^3.3.10"
  }
}
